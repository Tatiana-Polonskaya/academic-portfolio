{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:solid/typescript"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "off"
    },
    "parser": "@typescript-eslint/parser", // if not already added
    "plugins": ["solid"]
}
